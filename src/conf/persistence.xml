<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="EfemeridesTaurinasPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>Entities.Profesion</class>
    <class>Entities.Usuario</class>
    <class>Entities.Personaje</class>
    <class>Entities.Efemeride</class>
    <class>Entities.Trabajo</class>
    <class>entities.Fotos</class>
    <class>entities.Usuario</class>
    <class>entities.Efemeride</class>
    <class>entities.Personaje</class>
    <class>entities.Profesion</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/torosdb?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="toros"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="toros"/>
    </properties>
  </persistence-unit>
</persistence>
